{
  "name": "edge-utils",
  "version": "1.0.0",
  "description": "Platform-agnostic utilities for edge computing and serverless environments",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./cache": {
      "import": "./dist/cache.esm.js",
      "require": "./dist/cache.js"
    },
    "./rate-limiting": {
      "import": "./dist/rate-limiting.esm.js",
      "require": "./dist/rate-limiting.js"
    },
    "./security": {
      "import": "./dist/security.esm.js",
      "require": "./dist/security.js"
    },
    "./auth": {
      "import": "./dist/auth.esm.js",
      "require": "./dist/auth.js"
    },
    "./monitoring": {
      "import": "./dist/monitoring.esm.js",
      "require": "./dist/monitoring.js"
    },
    "./graphql": {
      "import": "./dist/graphql.esm.js",
      "require": "./dist/graphql.js"
    }
  },
  "keywords": [
    "edge",
    "serverless",
    "cloudflare",
    "vercel",
    "deno",
    "lambda",
    "edge-computing"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZFlareUI/Edge-Utils.git"
  },
  "bugs": {
    "url": "https://github.com/ZFlareUI/Edge-Utils/issues"
  },
  "homepage": "https://github.com/ZFlareUI/Edge-Utils#readme",
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "echo 'No linter configured'",
    "prepublishOnly": "npm run build && npm test"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "jest": "^29.0.0",
    "rollup": "^2.0.0",
    "rollup-plugin-terser": "^7.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@types/ajv": "^0.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "ajv": "^8.17.1",
    "crypto-js": "^4.2.0",
    "jsonwebtoken": "^9.0.2"
  }
}
